<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <!--<kr.gyjmoble.graphtest.GraphView android:id="@+id/GraphView"-->
    <!--xmlns:gyjmobile="http://schemas.android.com/apk/res/kr.gyjmoble.graphtest"-->
    <!--android:layout_width="match_parent" android:layout_height="match_parent"-->
    <!--gyjmobile:textColor="#ec4800" gyjmobile:textSize="15sp"-->
    <!--gyjmobile:textGap="2dp"-->
    <!--gyjmobile:lineColor="#0073cc" gyjmobile:lineThickness="20dp"/>-->
    <TextView
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="최종원님 소비패턴분석"
        android:textStyle="bold"
        android:textSize="20dp"
        android:textColor="#000000"
        android:layout_gravity="center"
        android:gravity="center"/>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp">

        <TextView
            android:id="@+id/textView2"
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:layout_alignParentBottom="true"
            android:layout_gravity="center"
            android:layout_weight="12"
            android:gravity="center"
            android:textColor="#000000"
            android:text="음식"
            android:textStyle="bold" />

        <LinearLayout
            android:id="@+id/linearlayout2"
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:layout_alignParentTop="true"
            android:layout_toEndOf="@+id/textView2">

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="200dp"
                android:layout_height="25dp"
                android:layout_alignBottom="@+id/imageView"
                android:layout_gravity="center_vertical"
                android:layout_toEndOf="@+id/textView2"
                android:layout_weight="8"
                android:background="#7381D1"
                android:gravity="center" />
        </LinearLayout>
        <ImageView
            android:id="@+id/imageView"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_toEndOf="@+id/linearlayout2"
            android:layout_weight="4"
            android:background="@null" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp">
        <TextView
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:text="음료"
            android:textStyle="bold"
            android:layout_gravity="center"
            android:textColor="#000000"
            android:gravity="center"
            android:id="@+id/textView3" />

        <LinearLayout
            android:id="@+id/linearlayout5"
            android:layout_width="100dp"
            android:layout_height="50dp"
            android:layout_alignParentTop="true"
            android:layout_toEndOf="@+id/textView3">

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="130dp"
                android:layout_height="25dp"
                android:layout_alignBottom="@+id/imageView"
                android:layout_gravity="center_vertical"
                android:layout_toEndOf="@+id/textView2"
                android:layout_weight="8"
                android:background="#CD6565"
                android:gravity="center" />
        </LinearLayout>
        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_toEndOf="@+id/linearlayout5"
            android:layout_weight="4"
            android:background="@null" />
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp">
        <TextView
            android:id="@+id/textView5"
            android:layout_width="50dp"
            android:textColor="#000000"
            android:layout_height="50dp"
            android:text="청과"
            android:textStyle="bold"
            android:layout_gravity="center"
            android:gravity="center"/>
        <LinearLayout
            android:id="@+id/linearlayout8"
            android:layout_width="130dp"
            android:layout_height="50dp"
            android:layout_alignParentTop="true"
            android:layout_toEndOf="@+id/textView5">

            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="130dp"
                android:layout_height="25dp"
                android:layout_alignBottom="@+id/imageView"
                android:layout_gravity="center_vertical"
                android:layout_toEndOf="@+id/textView2"
                android:layout_weight="8"
                android:background="#EBC452"
                android:gravity="center" />
        </LinearLayout>
        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_toEndOf="@+id/linearlayout8"
            android:layout_weight="4"
            android:background="@null" />
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp">
        <TextView
            android:id="@+id/textView6"
            android:layout_width="50dp"
            android:textColor="#000000"
            android:layout_height="50dp"
            android:text="의류"
            android:textStyle="bold"
            android:layout_gravity="center"
            android:gravity="center"/>

        <LinearLayout
            android:id="@+id/linearlayout9"
            android:layout_width="160dp"
            android:layout_height="50dp"
            android:layout_alignParentTop="true"
            android:layout_toEndOf="@+id/textView6">

            <ImageView
                android:id="@+id/imageView12"
                android:layout_width="160dp"
                android:layout_height="25dp"
                android:layout_alignBottom="@+id/imageView"
                android:layout_gravity="center_vertical"
                android:layout_toEndOf="@+id/textView6"
                android:layout_weight="8"
                android:background="#90C968"
                android:gravity="center" />
        </LinearLayout>
        <ImageView
            android:id="@+id/imageView13"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_toEndOf="@+id/linearlayout9"
            android:layout_weight="4"
            android:background="@null" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/relativelayout1"
        android:layout_width="match_parent"
        android:layout_height="50dp">
        <TextView
            android:id="@+id/textView8"
            android:textColor="#000000"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:text="기타"
            android:textStyle="bold"
            android:layout_gravity="center"
            android:gravity="center"/>

        <LinearLayout
            android:id="@+id/linearlayout3"
            android:layout_width="100dp"
            android:layout_height="50dp"
            android:layout_alignParentTop="true"
            android:layout_toEndOf="@+id/textView8">

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="100dp"
                android:layout_height="25dp"
                android:layout_alignBottom="@+id/imageView"
                android:layout_gravity="center_vertical"
                android:layout_toEndOf="@+id/textView8"
                android:layout_weight="8"
                android:background="#E585B0"
                android:gravity="center" />
        </LinearLayout>
        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_toEndOf="@+id/linearlayout3"
            android:layout_weight="4"
            android:background="@null" />
    </RelativeLayout>

    <TextView
        android:id="@+id/food"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:layout_gravity="center_vertical|right"
        android:gravity="right"
        android:paddingRight="10dp"
        android:paddingTop="30dp"
        android:text="음식  110,800원"
        android:textColor="#000000"

        android:textSize="12.5dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/drink"
        android:layout_width="match_parent"
        android:layout_height="25dp"
        android:layout_gravity="center_vertical|right"
        android:layout_toEndOf="@+id/relativelayout1"
        android:gravity="right"
        android:text="음료  55,400원"
        android:textColor="#000000"
        android:paddingRight="10dp"
        android:textSize="12.5dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/fruit"
        android:layout_width="match_parent"
        android:layout_height="25dp"
        android:layout_gravity="center_vertical|right"
        android:layout_toEndOf="@+id/drink"
        android:gravity="right"
        android:text="청과  72,050원"
        android:textColor="#000000"
        android:paddingRight="10dp"
        android:textSize="12.5dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/clothes"
        android:layout_width="match_parent"
        android:layout_height="25dp"
        android:layout_gravity="center_vertical|right"
        android:layout_toEndOf="@+id/fruit"
        android:gravity="right"
        android:text="의류 88,650원"
        android:paddingRight="10dp"
        android:textColor="#000000"
        android:textSize="12.5dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/etc"
        android:layout_width="match_parent"
        android:layout_height="25dp"
        android:layout_gravity="center_vertical|right"
        android:layout_toEndOf="@+id/clothes"
        android:gravity="right"
        android:paddingRight="10dp"
        android:text="기타 55,400"
        android:textColor="#000000"
        android:textSize="12.5dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/total"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingTop="20dp"
        android:layout_toEndOf="@+id/etc"
        android:layout_gravity="center_vertical|right"
        android:gravity="center"
        android:text="8월 총 소비량은 382,3300원입니다."
        android:textColor="#000000"
        android:textSize="25dp"
        android:textStyle="bold" />
</LinearLayout>

